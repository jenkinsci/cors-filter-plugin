<?jelly escape-by-default='true'?>
<j:jelly xmlns:f="/lib/form" xmlns:j="jelly:core">
  <f:section title="URL Blacklist">
    <f:entry field="enabled" title="Is Enabled">
      <f:checkbox/>
    </f:entry>
    <f:entry field="blackListPrefix" title="Blacklisted URL">
      <f:textarea/>
    </f:entry>
    <f:entry>
      <f:validateButton method="resetDefault" title="Reset Default"/>
    </f:entry>
  </f:section>
</j:jelly>
